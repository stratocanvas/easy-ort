{
	"name": "@stratocanvas/easy-ort",
	"version": "v1.1.1",
	"description": "Easy-to-use ONNX Runtime wrapper for common ML tasks",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs",
			"types": "./dist/index.d.ts"
		}
	},
	"files": [
		"dist",
		"README.md"
	],
	"scripts": {
		"build": "tsup",
		"test": "tsx test.ts",
		"test:watch": "tsx watch test.ts",
		"prepublishOnly": "npm run build"
	},
	"author": "Stratocanvas",
	"keywords": [
		"ONNX",
		"ONNXRuntime",
		"ONNX Runtime"
	],
	"license": "MIT",
	"dependencies": {
		"onnxruntime-node": "^1.19.2",
		"sharp": "^0.33.5"
	},
	"devDependencies": {
		"@types/node": "^20.8.7",
		"tsup": "^7.2.0",
		"tsx": "^4.7.1",
		"typescript": "^5.2.2"
	},
	"publishConfig": {
		"access": "public"
	}
}
